@Library('jenkins_shared_lib') _
pipeline{
        agent any 
        stages{

            stage('Git Checkout'){
                steps{
                    script{

                        gitCheckoutt()
                    }
                }

            }
           
            stage('Maven Unit Test'){
                steps{
                   

                        mvnTestt()
            
                }

            }
            stage('Maven Intergation Test'){
                steps{
                   

                    mvnIntergationTestt()
            
                }

            }
            

        }
    }